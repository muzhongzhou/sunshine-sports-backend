  },
  "scripts": {
    "start": "NODE_ENV=production node ./dist/index.js",
    "dev": "NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin test --ts --coverage",
    "lint": "eslint . --ext .ts",
    "bundle": "bundle && npm run build && ncc build bootstrap.js -o build",
    "bundle_start": "cross-env NODE_ENV=production node ./build/index.js",
    "build": "midway-bin build -c",
    "typeorm:generate": "npx ts-node ./node_modules/typeorm/cli.js migration:generate ./src/migration/init -d ./src/data-source.ts",
    "typeorm:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts"
  },
